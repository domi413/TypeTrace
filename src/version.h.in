/// Version information for TypeTrace (generated by CMake)

#ifndef VERSION_H
#define VERSION_H

/// Application version string
static constexpr char PROJECT_VERSION[] = "@PROJECT_VERSION@";

/// Application name
static constexpr char PROJECT_NAME[] = "@PROJECT_NAME@";

/// Git commit hash (if available)
static constexpr char PROJECT_GIT_COMMIT[] = "@GIT_COMMIT@";

/// Build type (Debug/Release)
static constexpr char PROJECT_BUILD_TYPE[] = "@CMAKE_BUILD_TYPE@";

#endif // VERSION_H
